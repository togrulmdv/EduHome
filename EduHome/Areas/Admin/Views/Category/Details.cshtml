@using EduHome.Areas.Admin.ViewModels.CategoryViewModels;

@{
    ViewData["Title"] = "Details";
}

@model DetailCategoryViewModel
<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">

        <div class="card-body" style="font-size:20px">

            <a class="btn btn-primary" asp-action="index">GO BACK</a>

           
            <h1>Name:@Model.Name</h1>
            @foreach(var course in Model.Courses)
            {
                <h1>Course:@course</h1>
            }

            <h1>Is Deleted:@Model.IsDeleted</h1
            <h1>CreatedBy:@Model.CreatedBy</h1>
            <h1>CreatedTime:@Model.CreatedDate</h1>
            <h1>UpdatedBy:@Model.UpdatedBy</h1>
            <h1>UpdatedTime:@Model.UpdatedDate</h1>



        </div>
    </div>
</div>